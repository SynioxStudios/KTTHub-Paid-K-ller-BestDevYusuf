local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local config = {
    whitelist = "https://pastebin.com/raw/bj3aQnNh",
    script = "https://raw.githubusercontent.com/SynioxStudios/SynioxKillSC-PA-DHUB-YusufDev-AutoKill/refs/heads/main/Synioxkill.txt"
}

local function StartCheck()
    local success, content = pcall(function() 
        return game:HttpGet(config.whitelist) 
    end)

    if success then
        local loadSuccess, whitelistTable = pcall(function() 
            return loadstring(content)() 
        end)

        if loadSuccess and type(whitelistTable) == "table" then
            local myName = LocalPlayer.Name:lower()
            local isAllowed = false

            for name, value in pairs(whitelistTable) do
                if name:lower() == myName and value == true then
                    isAllowed = true
                    break
                end
            end

            if isAllowed then
                -- âœ… OnaylandÄ±
                print("Access Granted ByYusuf!")
                loadstring(game:HttpGet(config.script))()
            else
                -- âŒ Sert Ä°ngilizce Mesaj (GÃ¶rÃ¼nme Garantili Format)
                local kickMessage = "No Whitelist! Where did you get this? Your account is mine now ğŸ’€"
                LocalPlayer:Kick(kickMessage)
            end
        else
            LocalPlayer:Kick("Database Error âš ï¸")
        end
    else
        LocalPlayer:Kick("Connection Error ğŸŒ")
    end
end

StartCheck()
