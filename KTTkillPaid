local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

-- Ayarlar
local config = {
    whitelist = "https://pastebin.com/raw/bj3aQnNh",
    script = "https://raw.githubusercontent.com/SynioxStudios/SynioxKillSC-PA-DHUB-YusufDev-AutoKill/refs/heads/main/Synioxkill.txt"
}

local function CheckWhitelist()
    -- Whitelist verisini Ã§ek
    local success, content = pcall(function()
        return game:HttpGet(config.whitelist)
    end)

    if success then
        -- Veriyi tabloya Ã§evir
        local loadSuccess, whitelistTable = pcall(function()
            return loadstring(content)()
        end)

        if loadSuccess and type(whitelistTable) == "table" then
            local myName = LocalPlayer.Name:lower()
            local isAuthorized = false

            -- Ä°sim kontrolÃ¼
            for authorizedName, value in pairs(whitelistTable) do
                if authorizedName:lower() == myName and value == true then
                    isAuthorized = true
                    break
                end
            end

            if isAuthorized then
                -- âœ… OnaylandÄ±: Scripti Ã§alÄ±ÅŸtÄ±r
                loadstring(game:HttpGet(config.script))()
            else
                -- âŒ Yetkisiz: Sadece emoji
                LocalPlayer:Kick("ğŸš«")
            end
        else
            -- âš™ï¸ Liste Bozuk: Sadece emoji
            LocalPlayer:Kick("âš™ï¸")
        end
    else
        -- ğŸŒ BaÄŸlantÄ± Yok: Sadece emoji
        LocalPlayer:Kick("ğŸŒ")
    end
end

-- BaÅŸlat
CheckWhitelist()
