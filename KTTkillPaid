local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local config = {
    whitelist = "https://pastebin.com/raw/bj3aQnNh",
    script = "https://raw.githubusercontent.com/SynioxStudios/SynioxKillSC-PA-DHUB-YusufDev-AutoKill/refs/heads/main/Synioxkill.txt"
}

local function StartCheck()
    local success, content = pcall(function() return game:HttpGet(config.whitelist) end)
    if success then
        local loadSuccess, whitelistTable = pcall(function() return loadstring(content)() end)
        if loadSuccess and type(whitelistTable) == "table" then
            local myName = LocalPlayer.Name:lower()
            local isAllowed = false

            for name, value in pairs(whitelistTable) do
                if name:lower() == myName and value == true then
                    isAllowed = true
                    break
                end
            end

            if isAllowed then
                loadstring(game:HttpGet(config.script))()
            else
                -- Emoji sonda olacak ≈üekilde, kƒ±sa ve √∂z ƒ∞ngilizce mesaj
                LocalPlayer:Kick("No Whitelist Found üö´")
            end
        else
            LocalPlayer:Kick("Data Error ‚ö†Ô∏è")
        end
    else
        LocalPlayer:Kick("Connection Error üåê")
    end
end

StartCheck()
